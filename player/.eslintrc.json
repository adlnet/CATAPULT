{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "es6": true,
        "mocha": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
    "no-return-await":"off",
    "no-inline-comments" :"off",
    "one-var-declaration-per-line" : "off",
    "no-return-assign": "off",
    "no-unused-expressions": "off"
    
    }
}
